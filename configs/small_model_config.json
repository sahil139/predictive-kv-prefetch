{
  "model_name": "microsoft/DialoGPT-medium",
  "batch_size": 1,
  "max_context_length": 2048,
  "quantization_config": {
    "load_in_8bit": true,
    "load_in_4bit": false
  },
  "device": "auto",
  "torch_dtype": "float16",
  "trust_remote_code": true,
  "use_cache": true,
  "do_sample": false,
  "temperature": 1.0,
  "top_p": 1.0,
  "top_k": 50,
  "repetition_penalty": 1.0,
  "max_new_tokens": 50,
  "pad_token_id": null,
  "eos_token_id": null,
  "experiment_name": "small_model_test",
  "output_dir": "logs",
  "log_level": "INFO",
  "save_model": false,
  "warmup_runs": 2,
  "benchmark_runs": 5
}
